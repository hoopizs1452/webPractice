return true;}
function cryptData(strData)
{var strSessionKey=$.jCryption.encrypt(Math.floor((Math.random()*1000000)+1).toString(),(new Date).getTime().toString());var dfdCrypt=$.Deferred();if(strData=='')
{dfdCrypt.resolve('');}
else
{$.getJSON('/ecapi/cryptionapi/v1/index.php/jcryption/publickey?'+(new Date).getTime().toString()).done(function(data)
{$.jCryption.crypt.setKey(data.Publickey);var strEncryptedString=$.jCryption.crypt.encrypt(strSessionKey);$.ajax({url:'/ecapi/cryptionapi/v1/index.php/jcryption/handshake',dataType:'json',type:'POST',data:{Key:strEncryptedString}}).done(function(data)
{if($.jCryption.challenge(data.Challenge,strSessionKey))
{dfdCrypt.resolve($.jCryption.encrypt(strData,strSessionKey));}
else
{dfdCrypt.resolve(false);}}).fail(function()
{dfdCrypt.resolve(false);});}).fail(function()
{dfdCrypt.resolve(false);});}
return dfdCrypt;}
