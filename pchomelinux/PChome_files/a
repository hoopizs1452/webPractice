{switch(objRs.Status)
{case'Done':switch(strAction)
{case'login':$(document).trigger('_login',[objData]);break;case'signup':$(document).trigger('_signup',[objData]);break;}
break;case'Fail':$(document).trigger('_displayReCaptchaError');break;default:if(intRetry>=6)
{$(document).trigger('_displayReCaptchaError');}
