{dfdMemberId.resolve('');}}).fail(function()
{dfdMemberId.resolve('');});}
return dfdMemberId;}
function getCookie(strName)
{var strNameEQ=strName+"=";var ca=document.cookie.split(';');for(var i=0;i<ca.length;i++)
{var c=ca[i];c=c.replace(/^\s+/,'');if(c.indexOf(strNameEQ)==0)
{return c.substring(strNameEQ.length,c.length);}}
return null;}
function asc(strText)
{var asciiTable="!\"#$%&\'()*+,-./0123456789:;<=>?@ABCDEFGHIJKLMNOPQRSTUVWXYZ[\\]^_`abcdefghijklmnopqrstuvwxyz{|}~ ";var big5Table="%uFF01%u201D%uFF03%uFF04%uFF05%uFF06%u2019%uFF08%uFF09%uFF0A%uFF0B%uFF0C%uFF0D%uFF0E%uFF0F%uFF10%uFF11%uFF12%uFF13%uFF14%uFF15%uFF16%uFF17%uFF18%uFF19%uFF1A%uFF1B%uFF1C%uFF1D%uFF1E%uFF1F%uFF20%uFF21%uFF22%uFF23%uFF24%uFF25%uFF26%uFF27%uFF28%uFF29%uFF2A%uFF2B%uFF2C%uFF2D%uFF2E%uFF2F%uFF30%uFF31%uFF32%uFF33%uFF34%uFF35%uFF36%uFF37%uFF38%uFF39%uFF3A%uFF3B%uFF3C%uFF3D%uFF3E%uFF3F%u2018%uFF41%uFF42%uFF43%uFF44%uFF45%uFF46%uFF47%uFF48%uFF49%uFF4A%uFF4B%uFF4C%uFF4D%uFF4E%uFF4F%uFF50%uFF51%uFF52%uFF53%uFF54%uFF55%uFF56%uFF57%uFF58%uFF59%uFF5A%uFF5B%uFF5C%uFF5D%uFF5E%u3000";var result="";for(var i=0;i<strText.length;i++)
{var val=escape(strText.charAt(i));var j=big5Table.indexOf(val);result+=(((j>-1)&&(val.length==6))?asciiTable.charAt(j/6):strText.charAt(i));}
return result;}
function isphone(strAccount)
{if(strAccount.match(/^09\d{8}$/))
{return true;}
return false;}
function alertmsg(strMsg,elTarget,callback)
{if(typeof elTarget!='undefined'&&elTarget.length>0)
{elTarget.closest('dd').find('.list_box:eq(0)').html(strMsg);if(typeof callback!='undefined')
{callback();}}}
function popmsg(strMsg,callback)
{$('#origshadow').removeClass('fade-ready fade-out').addClass('fade-in');$('body').addClass('no-scroll');$('.modal-pure').find('.msg_box').html(strMsg).end().removeClass('fade-ready fade-out').addClass('fade-in');$(document).data('last','');$(document).data('alertcallback','');if(typeof callback!='undefined')
{$(document).data('alertcallback',callback);}}
function errorMsg(strCode,elTarget)
{switch(strCode)
{case'400-02-004':$(document).data('alertcallback','');$('#origshadow').trigger('click');popmsg('此帳號已存在，請直接登入！',function()
{$(document).data('alertcallback','');$('#loginAcc').val($('.signupAccount:visible').val());$('.signupAccount, .signupPassword, .signupPassword_C').val('');$('.tabs li:eq(0)').click();$('html,body').scrollTop(0);$('#loginPwd').focus();});break;case'400-02-003':var _account=$('.signupAccount:visible').val();alertmsg('<li><span class="attention">{0}</span></li>'.format(isphone(_account)?'輸入的手機號碼格式有誤':'Email&nbsp;帳號不能含有單引號、雙引號、逗號、分號、特殊字元、中文或&nbsp;Email&nbsp;格式錯誤'),$('.signupAccount:visible'),function()
{$('.signupAccount:visible').focus();});break;case'400-02-005':alertmsg('<li><span class="attention">請輸入6碼 ~ 16碼的密碼，密碼限用英文、數字或底線</span></li>',$('.signupPassword:visible'),function()
{$('.signupPassword:visible').focus();});break;case'403-02-007':$(document).data('alertcallback','');$('#origshadow').trigger('click');alertmsg('<li><span class="attention">請稍候再試，謝謝。</span></li>',elTarget,function()
{elTarget.focus();});break;case'403-02-009':alertmsg('<li><span class="attention">帳號不存在，請重新輸入</span></li>',$('.signupAccount:visible'),function()
{$('.signupAccount:visible').focus();});break;case'403-02-010':alertmsg('<li><span class="attention">輸入的密碼需與Email信箱相同！</span></li>',$('.signupPassword:visible'),function()
{$('.signupPassword:visible').focus();});break;case'403-02-004':case'403-06-004':if(elTarget.hasClass('OTPCaptcha'))
{alertmsg('<li><span class="attention">驗證碼發送過於頻繁，請等待8小時後再重試</span></li>',elTarget,function()
{elTarget.focus();});}
else
{popmsg('驗證碼發送過於頻繁，請等待8小時後再重試');}
