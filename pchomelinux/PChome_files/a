{popmsg('驗證碼錯誤次數過於頻繁，請等待30分鐘後再重試');}
break;case'403-02-008':if(elTarget.hasClass('OTPCaptcha'))
{alertmsg('<li><span class="attention">驗證碼發送過於頻繁，請等待明天再重試</span></li>',elTarget,function()
{elTarget.focus();});}
else
{popmsg('驗證碼發送過於頻繁，請等待明天再重試');}
break;case'403-02-006':case'403-06-006':alertmsg('<li><span class="attention">驗證碼輸入錯誤，請重新輸入</span></li>',elTarget,function()
{elTarget.focus();});break;case'403-03-007':alertmsg('<li><span class="attention">帳號密碼錯誤，請重新輸入</span><span id="toreg" class="alink">註冊請按此</span></li>',elTarget,function()
{$('#loginPwd').focus();$('#toreg').on('click',function()
{$('.list_box .attention').parent().remove();if(isphone($('#loginAcc').val()))
