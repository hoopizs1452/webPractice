@model WEB.Models.ManageExpenseTypeModel

@*建立刪除消費種類表單*@
@using (Html.BeginForm("DeleteExpenseType", "Drink"))
{
    <table>
        <tr>
            <td>
                @Html.LabelFor(m => m.expenseTypeNumber, new { @class = "label label-primary" })
            </td>
            <td>
                @Html.TextBoxFor(m => m.expenseTypeNumber)
                @Html.ValidationMessageFor(m => m.expenseTypeNumber, "", new { @class = "text-danger" })
                <input type="submit" name="btnSend" value="刪除" class="btn btn-success">
            </td>
        </tr>
    </table>
}
